   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../img/singleGameBackground.jpg" />
         </image>
      </ImageView>
      <Button fx:id="backButton" layoutX="622.0" layoutY="22.0" mnemonicParsing="false" onAction="#backButtonClicked" opacity="0.0" prefHeight="58.0" prefWidth="156.0" text="Button" />
      <Button layoutX="82.0" layoutY="401.0" mnemonicParsing="false" onAction="#loadButtonClicked" opacity="0.0" prefHeight="107.0" prefWidth="282.0" text="Button" />
      <Button fx:id="newGame" layoutX="432.0" layoutY="398.0" mnemonicParsing="false" onAction="#newGameClicked" opacity="0.0" prefHeight="114.0" prefWidth="290.0" text="Button" />
   </children>
</AnchorPane>
